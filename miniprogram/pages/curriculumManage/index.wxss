}
.content .listCustomDown .tagList {
  margin-left: 5rpx;
}
.content .listCustomDown .numdes {
  text-align: right;
  display: flex;
  flex-direction: row;
  margin-bottom: 5rpx;
}
.content .listCustomDown .numdes .coursePrice {
  color: red;
  font-size: 28rpx;
  font-weight: bold;
  flex: 3;
  text-align: left;
}
.content .listCustomDown .numdes .courseCategory {
  flex: 1;
}
.content .listCustomDown .van-tag {
  background-color: red;
}
.content .listCustomDown .secbutton {
  margin-left: 5rpx;
}
.lastContent {
  margin-bottom: 130rpx;
}
.fircon {
